<html>
<head>
    <title>í™ˆ | mago market </title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <style>
        .scroll{
            text-align: center;
            border: 1px solid rgb(255, 255, 255);
            border-radius: 10px;
            padding: 60px 40px 50px; 
            height: 800px;
            width: 500px;
            margin-left: auto;
            margin-right: auto;
        }

        .jiji{
            max-width:100%;
            max-height:100%;
            object-fit: scale-down;
        }

        .b{
            width: 100px;
            height: 100px;
        }
    </style>
</head>
<body>
    <div class="scroll" style="overflow-x:hidden" style="text-align: center;" class="jiji" id="scroll">
        <% images.forEach(image => { %>
            <img class="jiji" src="<%= image %>" style="text-align: center;">
            <br>
        <% }); %>
    </div>
    <form id="picture" style="text-align: center;">
        <img src="../images/chat.png" width="50px" height="50px" onclick="location.href='extra-chat.html'">
        <img src="../images/plus.jpg" width="50px" height="50px" onclick="location.href='extra-send.html'">
        <img src="../images/profile.png" width="50px" height="50px" onclick="location.href='extra-profile.html'">
    </form>
    <footer>
        <img src="../images/exit.png" width="50px" height="50px" id="exit" name="exit">
    </footer>
</body>
</html>